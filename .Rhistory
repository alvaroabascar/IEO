oneExp <- rowMeans(exprs(eset[, eset$characteristics_ch1.6 == "1"]))
par(mfrow = c(1, 2))
plot(zeroExp, oneExp, xlab = "Zero", ylab = "One", pch = ".", cex = 4, las = 1)
